{
	"schema_version": 1,
	"quilt_loader": {
		"group": "${maven_group}",
		"id": "stairautojump",
		"version": "${version}",
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "io.github.therookiecoder.stairautojump.StairAutoJump"
		},
		"metadata": {
			"name": "StairAutoJump",
			"description": "Turns on auto jump when in/on stairs",
			"contributors": {
				"theRookieCoder": "Owner",
				"RaptaG": "Contributor"
			},
			"contact": {
				"homepage": "https://modrinth.com/mod/stairautojump",
				"issues": "https://github.com/theRookieCoder/StairAutoJump/issues",
				"sources": "https://github.com/theRookieCoder/StairAutoJump"
			},
			"license": "MPL-2.0",
			"icon": "assets/stairautojump/icon.png"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": "*"
			},
			{
				"id": "minecraft",
				"versions": "*"
			}
		]
	},
	"minecraft": {
		"environment": "client"
	},
	"mixin": "stairautojump.mixins.json"
}
